class GfG
{
	public int[] formatArray(int[] a,int n)
        {
        // add code here.
        
        int temp=0;
        for(int i=0;i<=n-2;i+=2)
        {
            if(a[i]>a[i+1])
            {
               temp=a[i];
               a[i]=a[i+1];
               a[i+1]=temp;
            }
        }
        return a;
        }
}
