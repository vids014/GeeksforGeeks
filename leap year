/*package whatever //do not write package name here */

import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		//code
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int n=sc.nextInt();
		    if((n%400==0)||((n%4==0) && (n%100!=0))){
		    System.out.println("Yes");
		    }else{
		    System.out.println("No");
		}
		}
	}
}
