/*package whatever //do not write package name here */

import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		//code
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int n=sc.nextInt();
		    int count=0;
		    while(n!=0){
		        if(n%2!=0){
		            n=n-1;
		        }else{
		            n=n/2;
		        }
		        count++;
		    }
		    System.out.println(count);
		}
	}
}
